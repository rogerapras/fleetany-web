{
    "name": "alientronics/fleetany-web",
    "description": "An open source fleet management web system",
    "keywords": ["alientronics", "fleetany", "web"],
    "license": "AGPL-3.0",
    "type": "project",
    "repositories": [
        {
            "type": "package",
            "package": {
                "version": "dev-master",
                "name": "alientronics/cached-eloquent-user",
                "source": {
                    "url": "https://github.com/alientronics/cached-eloquent-user.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "version": "dev-master",
                "name": "alientronics/laravel-acl",
                "source": {
                    "url": "https://github.com/alientronics/laravel-acl.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "version": "dev-master",
                "name": "alientronics/fleetany-web-pages",
                "source": {
                    "url": "https://github.com/alientronics/fleetany-web-pages.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "version": "dev-master",
                "name": "alientronics/fleetany-web-reports",
                "source": {
                    "url": "https://github.com/alientronics/fleetany-web-reports.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "version": "dev-master",
                "name": "alientronics/fleetany-web-geofence",
                "source": {
                    "url": "https://github.com/alientronics/fleetany-web-geofence.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "version": "dev-master",
                "name": "alientronics/fleetany-web-driver",
                "source": {
                    "url": "https://github.com/alientronics/fleetany-web-driver.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "version": "dev-master",
                "name": "alientronics/fleetany-web-attributes",
                "source": {
                    "url": "https://github.com/alientronics/fleetany-web-attributes.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "version": "dev-master",
                "name": "alientronics/fleetany-web-admin",
                "source": {
                    "url": "https://github.com/alientronics/fleetany-web-admin.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "laravel/framework": "5.2.*",
        "laravelcollective/html": "5.2.*",
        "prettus/l5-repository": "dev-master",
        "prettus/laravel-validation": "1.1.*",
        "laravel/socialite": "^2.0",
		"doctrine/dbal": "v2.4.2",
        "alientronics/cached-eloquent-user": "dev-master",
        "alientronics/laravel-acl": "dev-master",
        "alientronics/fleetany-web-pages": "dev-master",
        "alientronics/fleetany-web-reports": "dev-master",
        "alientronics/fleetany-web-geofence": "dev-master",
        "alientronics/fleetany-web-driver": "dev-master",
        "alientronics/fleetany-web-attributes": "dev-master",
        "alientronics/fleetany-web-admin": "dev-master",
        "barryvdh/laravel-debugbar": "^2.2",
        "caouecs/laravel-lang": "^3.0"
    },
    "require-dev": {
        "symfony/dom-crawler": "~3.0",
        "symfony/css-selector": "~3.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~4.0",
        "phpspec/phpspec": "~2.1",
        "squizlabs/php_codesniffer": "^2.3",
        "phploc/phploc": "*",
        "sebastian/phpcpd": "*",
        "phpmd/phpmd": "*",
        "codeclimate/php-test-reporter": "dev-master"
    },
    "autoload": {
        "classmap": [
            "database",
            "vendor/alientronics/cached-eloquent-user/src/",
            "vendor/alientronics/laravel-acl/src/Kodeine/Acl/",
            "vendor/alientronics/fleetany-web-pages/src/",
            "vendor/alientronics/fleetany-web-reports/src/",
            "vendor/alientronics/fleetany-web-geofence/src/",
            "vendor/alientronics/fleetany-web-driver/src/",
            "vendor/alientronics/fleetany-web-attributes/src/",
            "vendor/alientronics/fleetany-web-admin/src/"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/AcceptanceTestCase.php",
            "tests/UnitTestCase.php"
        ],
        "psr-4": {
            "Tests\\": "tests/",
			"Alientronics\\FleetanyWebPages\\": "src/Alientronics/FleetanyWebPages",
			"Alientronics\\FleetanyWebReports\\": "src/Alientronics/FleetanyWebReports"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan vendor:publish --provider='Kodeine\\Acl\\AclServiceProvider'",
            "php artisan vendor:publish --provider='Alientronics\\FleetanyWebGeofence\\FleetanyWebGeofenceServiceProvider'",
            "php artisan vendor:publish --provider='Alientronics\\FleetanyWebDriver\\FleetanyWebDriverServiceProvider'",
            "php artisan vendor:publish --provider='Alientronics\\FleetanyWebAttributes\\FleetanyWebAttributesServiceProvider'",
            "php artisan vendor:publish --provider='Alientronics\\FleetanyWebAdmin\\FleetanyWebAdminServiceProvider'",
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "pre-update-cmd": [
            "php artisan clear-compiled"
        ],
        "post-update-cmd": [
            "php artisan optimize"
        ],
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
